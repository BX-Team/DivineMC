     public boolean canMate(Animal other) {
         if (other == this) {
             return false;
-        } else if (!this.isTame()) {
-            return false;
         } else if (!(other instanceof Wolf)) {
             return false;
         } else {
-            Wolf entitywolf = (Wolf) other;
